<div class="inner_cell">
    <div class="input_area">
<div class=" highlight hl-ipython3"><pre><span></span><span class="c1">#sum of squared residuals</span>
<span class="n">ssr_unrestricted</span><span class="o">=</span><span class="n">res4</span><span class="o">.</span><span class="n">ssr</span>
<span class="n">ssr_restricted</span><span class="o">=</span><span class="n">res5</span><span class="o">.</span><span class="n">ssr</span>

<span class="n">f_testa</span><span class="o">=</span><span class="p">(</span><span class="n">ssr_restricted</span> <span class="o">-</span> <span class="n">ssr_unrestricted</span><span class="p">)</span><span class="o">/</span><span class="mi">10</span>
<span class="n">f_testb</span><span class="o">=</span><span class="n">ssr_unrestricted</span><span class="o">/</span><span class="mi">524</span>
<span class="n">f_test</span><span class="o">=</span><span class="n">f_testa</span><span class="o">/</span><span class="n">f_testb</span>

<span class="nb">print</span><span class="p">(</span><span class="s2">"F-Test for Joint Significance:"</span><span class="p">,</span><span class="n">f_test</span><span class="p">,</span><span class="s1">'with p&gt;.05 indicates joint significance'</span><span class="p">)</span>
</pre></div>

</div>
</div>
</div>

</div>
