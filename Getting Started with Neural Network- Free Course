.spinner .circle { overflow: hidden; }

.spinner .circle-inner { width: 100%; height: 200%; transform: rotate(45deg); border-radius: 50%; animation-name: anim-circle-1; animation-duration: 1s; animation-timing-function: cubic-bezier(0.25, 0.1, 0.5, 1); animation-iteration-count: infinite; animation-direction: alternate; }

.spinner .circle-2 { transform: rotate(180deg); }

.spinner .circle-2 .circle-inner { animation-name: anim-circle-2; }

